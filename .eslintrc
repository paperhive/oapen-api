{
  "extends": "airbnb/base",
  "ecmaFeatures": {
    "generators": true
  },
  "rules": {
    "object-curly-spacing": 0,
    "max-len": 0,
    "newline-per-chained-call": 0,
    "no-cond-assign": [2, "except-parens"],
    "no-confusing-arrow": 0,
    "no-param-reassign": [2, {"props": false}]
  }
}
